{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-daemon",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint:fix": "eslint *.ts --fix",
    "migrate:users": "npm run --workspace=users migrate",
    "migrate:tracks": "npm run --workspace=tracks migrate",
    "migrate:artists": "npm run --workspace=artists migrate",
    "generate:artists": "npm run --workspace=artists generate",
    "generate:users": "npm run --workspace=users generate",
    "generate:tracks": "npm run --workspace=tracks generate"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "name": "project",
  "packageManager": "npm@8.1.2",
  "workspaces": [
    "apps/**",
    "packages/*"
  ],
  "dependencies": {
    "dotenv-cli": "^7.2.1"
  }
}
